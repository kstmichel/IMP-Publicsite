<header class="header cf">
    <div class="header-inner">
        <div class="header-inner--logo col-lg-3 col-md-3 col-sm-3 col-xs-12">
            {% block branding %}
                {% embed "@atoms/_branding.twig" with {
                url: url('<front>'),
                logo_img_src: paths.assets ~ '/IMP_logo.png',
                site_name: '',
                site_slogan: '',
                } %}
                  {% endembed %}
            {% endblock branding%}
        </div>

        <div class="header-inner--nav col-lg-8 col-md-8 col-sm-8 col-xs-12">
            {% block primary_menu %}
                {% if primary_menu %}
                     {% include "@organisms/_navbar.twig"  with primary_menu %}
                {% endif %}
            {% endblock %}

            <div>
            {% block secondary_menu %}
            {% endblock secondary_menu%}
            </div>
        </div>
    </div>
</header>
